{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "*.js",
      "./src/**/*.test.tsx",
      "node_modules/**/*.ts",
      "styleguide/**/*.*",
      "./docs/**/*.*",
      "styleguide.*.js"
    ]
  },
  "rules": {
    "interface-name": false,
    "ordered-imports": false,
    "no-console": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "forin": false
  }
}
